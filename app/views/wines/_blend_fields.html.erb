<div class="my-1 border">
    <div class="form-group">
        <%= f.label :name, "Select Strain: " %> 
        <%= f.collection_select :strain_id, @strains, :id, :name%>
    </div>

    <div class="form-group">
        <%= f.label :percent, "Strain percent: "%>
        <%= f.number_field :percent, in: 0.1..100.0, step: 0.1, placeholder: '0.1 to 100.0' %>
    </div>
</div>

